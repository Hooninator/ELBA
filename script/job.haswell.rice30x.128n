#!/bin/bash

#SBATCH -N 128
#SBATCH -C haswell
#SBATCH -q regular
#SBATCH -J ELBA.rice30x.haswell.128n
#SBATCH --error=ELBA.rice30x.haswell.128n.%j.err
#SBATCH --output=ELBA.rice30x.haswell.128n.%j.out
#SBATCH --mail-user=gabe.h.raulet@gmail.com

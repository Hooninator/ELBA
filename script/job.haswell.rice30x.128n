srun -n 4096 -c 2 --cpu_bind=cores ./elba_rice30x -i $READS/SC22/rice_hifi_30x.fa -k 31 --idxmap 128.idxmap -c 638244 --alph dna --af ELBA.rice_30x.haswell.128.result -s 1 -O 100000 --afreq 100000 --xa 15
